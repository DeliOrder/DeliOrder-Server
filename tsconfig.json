{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "noEmitOnError": false,
    "preserveValueImports": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}
